const userModel = require('../models/Users');

const adminProfile = async(req, res) => {
    const result = await userModel.find({ role: req.body.role }, "-role");
    try {
        if(result) {
            res.status(200).json({
                msg: "admin details",
                data: result
            });
        }
    } catch (err) {
        res.status(401).json({
            msg: err.message
        });
    }
}

module.exports = {
    adminProfile,
}